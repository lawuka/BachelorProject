    //
    // All X, Y and Radius measures, have to be in 1:1mm (millimeter)
    // All Angle measures have to be in degrees
    //

    *****************************
    *                           *
    * Basic Internal Components *
    *                           *
    *****************************

    <FlowLine>
        <Start>
            <X></X>
            <Y></Y>
        </Start>
        <End>
            <X></X>
            <Y></Y>
        </End>
    </FlowLine>

