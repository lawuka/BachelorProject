    // READ ME:
    //
    // All X, Y and Radius measures, have to be in 1:1mm (millimeter)
    // All Angle measures have to be in degrees
    //

    ******************************
    *                            *
    * Atomic Internal Components *
    *                            *
    ******************************

    <FlowLine>
        <Start>
            <X></X>
            <Y></Y>
        </Start>
        <End>
            <X></X>
            <Y></Y>
        </End>
    </FlowLine>

    <FlowCircle>
        <Center>
            <X></X>
            <Y></Y>
        </Center>
        <Radius></Radius>
        <Valves>
            <Angle></Angle>
        </Valves>
    </FlowCircle>

    <FlowHole>
        <Center>
            <X></X>
            <Y></Y>
    </FlowHole>

    ******************************
    *                            *
    * Atomic External Components *
    *                            *
    ******************************

    <ConnectionPoint>
        <X></X>
        <Y></Y>
    </ConnectionPoint>

    <InputConnectionPoint>
        <X></X>
        <Y></Y>
    </InputConnectionPoint>

    <OutputConnectionPoint>
        <X></X>
        <Y></Y>
    </OutputConnectionPoint>

    *****************************
    *                           *
    * Atomic Control Components *
    *                           *
    *****************************

    <Valve>
        <X></X>
        <Y></Y>
        <Rotation></Rotation>
    </Valve>

    ****************************
    *                          *
    *  Basic Component Layout  *
    *                          *
    ****************************

    <Component>
        <Type></Type>
        <Size>
            <X></X>
            <Y></Y>
        </Size>
        <External>
            <ConnectionPoint/>         \
            <InputConnectionPoint/>     > All are not needed
            <OutputConnectionPoint/>   /
        </External>
        <Internal>
            <FlowLine/>     \
            <FlowCircle/>    \
            <FlowHole/>       \
            <Component1/>      \
            <Component2/>       > All are not needed
            .                  /
            .                 /
            .                /
            <ComponentN/>   /
        </Internal>
        <Control>
            <Valve/>   > A valve is not mandatory
                       > Do not add valves for valves on FlowCircle - are added automatically
                       > Only add valve for FlowLines, inherited components inherit their own valves
        </Control>
    </Component>

    -->

